    android:layout_height="match_parent"
    android:background="@drawable/med"
    tools:context="com.example.uddishverma22.hint.Fragments.Fragment9">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv9"
            android:layout_margin="20dp"
